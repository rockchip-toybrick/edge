



partial modifier_keys
xkb_symbols "rctrl_switch" {
 key <RCTL> {
 type[Group1]="ONE_LEVEL",
 symbols[Group1] = [ ISO_Level5_Shift ]
 };
 include "level5(modifier_mapping)"
};


partial modifier_keys
